<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="220.0" prefWidth="313.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Product">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TextField fx:id="nameTextfield" promptText="Product name" />
      <Text fx:id="errorName" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" />
      <TextField fx:id="characteristicsTextfield" promptText="Product characteristics" />
      <Text fx:id="errorCharacteristics" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" />
      <TextField fx:id="categoryTextfield" promptText="Category" />
      <Text fx:id="errorCategory" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" />
      <Button fx:id="createButton" mnemonicParsing="false" text="Create" />
      <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
   </children>
</VBox>
